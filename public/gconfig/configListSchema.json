{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "list": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "pattern": "^[A-Za-z 0-9]{3,}$"
                    },
                    "description": {
                        "type": "string",
                        "pattern": "^[A-Za-z 0-9]*$"
                    },
                    "rawUrl": {
                        "type": "string",
                        "pattern": "^https://raw.githubusercontent.com/.+?(?=\\.json)"
                    }
                }
            }
        }
    }
}