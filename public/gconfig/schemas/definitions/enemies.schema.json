{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "schemas/gconfig/definitions/enemies.schema.json",
    "type": "object",
    "definitions": {
        "enemies": {
            "type": "object",
            "required": [ "enemyList" ],
            "properties": {
                "enemyList": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "integer",
                        "minimum": 1
                    }
                }
            }
        }
    }
}